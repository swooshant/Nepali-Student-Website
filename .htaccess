RewriteRule ^events/?$ app/controller/siteController.php?action=events [QSA,L]
RewriteRule ^gallery/?$ app/controller/siteController.php?action=gallery [QSA,L]
RewriteRule ^members/?$ app/controller/siteController.php?action=members [QSA,L]
RewriteRule ^visit/?$ app/controller/siteController.php?action=visit [QSA,L]


#RewriteRule ^processLogin/?$ app/controller/siteController.php?action=processLogin [QSA,L]
