Options -Indexes
Require all granted

RewriteEngine on

# For custom 404 error page.
# ErrorDocument 404 /app/controller/error.php

# For redirections
# Redirect /old_dir/ http://www.yourdomain.com/new_dir/index.html

# Site
RewriteRule ^$ app/controller/siteController.php?action=home [QSA,L]
